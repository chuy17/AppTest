<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jesusramirez.proyecto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.Login" />
        <activity
            android:name=".views.CreateAccount"
            android:parentActivityName=".views.Login" />
        <activity android:name=".Toolbar" />
        <activity
            android:name=".views.HomeMaestro"
            android:label="@string/title_activity_home_maestro"
            android:theme="@style/AppTheme" />
        <activity android:name=".views.PantallaInicial">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.HomeAlumno"
            android:label="@string/title_activity_home_alumno"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>